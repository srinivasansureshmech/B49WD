// Using for loop
console.log("Using for loop:");
for (const key in resumeData) {
  console.log(key, ":", resumeData[key]);
}

// Using for...in loop
console.log("\nUsing for...in loop:");
for (const key in resumeData) {
  if (resumeData.hasOwnProperty(key)) {
    console.log(key, ":", resumeData[key]);
  }
}

// Using for...of loop (for arrays)
console.log("\nUsing for...of loop for skills:");
for (const skill of resumeData.skills) {
  console.log(skill);
}

console.log("\nUsing for...of loop for languages:");
for (const language of resumeData.languages) {
  console.log(language);
}

console.log("\nUsing for...of loop for interests:");
for (const interest of resumeData.interests) {
  console.log(interest);
}

// Using forEach loop (for arrays)
console.log("\nUsing forEach loop for work experience:");
resumeData.work_experience.forEach((experience, index) => {
  console.log(`Experience ${index + 1}:`);
  console.log("Title:", experience.title);
  console.log("Company:", experience.company);
  console.log("Location:", experience.location);
  console.log("Start Date:", experience.start_date);
  console.log("End Date:", experience.end_date);
  console.log("Description:", experience.description);
});
