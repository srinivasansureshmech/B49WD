class Movie {
  constructor(title, studio, rating = "PG") {
    this.title = title;
    this.studio = studio;
    this.rating = rating;
  }
}
class Movie {
  // ... (Constructor from part a)

  static getPG(movies) {
    return movies.filter(movie => movie.rating === "PG");
  }
}
const casinoRoyale = new Movie("Casino Royale", "Eon Productions", "PG-13");
